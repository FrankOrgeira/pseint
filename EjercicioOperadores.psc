Proceso EjercicioOperadores
	Definir a,b Como Entero;
	Definir resultado Como Logico;
	
	// Caso a > b
	a<-50;
	b<-10;
	
	resultado <- ((3+5*8)<3 Y (((-6/3)*4)+2<2)) O (a>b);
	
	Escribir "Sí a es mayor a b el operador lógico es ",resultado;
	
	// Caso a < b
	a<-20;
	b<-40;
	
	resultado <- ((3+5*8)<3 Y (((-6/3)*4)+2<2)) O (a>b);
	
	Escribir "Sí a es menor a b el operador lógico es ",resultado;
	
	
FinProceso